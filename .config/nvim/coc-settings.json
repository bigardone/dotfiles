{
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnSaveFiletypes": [
    "elixir",
    "eelixir",
    "heex",
    "elm",
    "json",
    "js",
    "javascript",
    "ex",
    "heex",
    "lua",
    "go",
    "sql",
    "gleam",
    "hcl",
    "markdown",
  ],
  "workspace.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json",
    "tailwind.config.js",
    "mix.exs"
  ],
  "markdownlint.onSave": true,
  "markdownlint.onOpen": false,
  "markdownlint.config": {
    "default": true,
    "line_length": true
  },
  "coc.preferences.useQuickfixForLocations": true,
  "codeLens.enable": true,
  "css.validate": false,
  "diagnostic.messageTarget": "float",
  "diagnostic.enableMessage": "always",
  "diagnostic.checkCurrentLine": true,
  "emmet.includeLanguages": {
    "eelixir": "html",
    "heex": "html"
  },
  "eslint.autoFixOnSave": false,
  "suggest.languageSourcePriority": 100,
  "suggest.localityBonus": false,
  "suggest.minTriggerInputLength": 1,
  "suggest.noselect": false,
  "suggest.removeDuplicateItems": true,
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "/Users/ricardogarciavega/.nvim/plugged/vim-snippets/UltiSnips"
  ],
  "sql.lintOnSave": true,
  "tailwindCSS.headwind.runOnSave": true,
  "tailwindCSS.headwind.prependCustomClasses": true,
  "tailwindCSS.includeLanguages": {
    "eelixir": "html",
    "elixir": "html",
    "heex": "html"
  },
  "languageserver": {
    "nextLS": {
      "command": "~/projects/elixir/next-ls/burrito_out/next_ls_darwin_arm64",
    },
    "elixirLS": {
      "command": "~/projects/elixir/elixir-ls/release/language_server.sh",
      "filetypes": [
        "elixir",
        "eelixir",
        "heex"
      ],
      "rootPatterns": [
        "mix.exs"
      ],
      "settings": {
        "elixirLS.autoBuild": true,
        "elixirLS.dialyzerEnabled": true,
        "elixirLS.suggestSpecs": false,
        "elixirLS.autoInsertRequiredAlias": false,
      }
    },
    "terraformLS": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "lexicalLS": {
      "command": "~/projects/elixir/lexical/_build/dev/package/lexical/bin/start_lexical.sh",
    },
    "gleam": {
      "command": "gleam",
      "args": [
        "lsp"
      ],
      "filetypes": [
        "gleam"
      ]
    }
  },
  "yaml.format.enable": true,
  "snippets.ultisnips.pythonPrompt": false
}
